<Window x:Class="ForCompanyMeetingProject.View.Windows.ShowingProjectWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ForCompanyMeetingProject.View.Windows"
        mc:Ignorable="d" WindowStartupLocation="CenterScreen"
        Title="Просмотр проекта" Height="600" Width="500">
    <Grid>
        <WebBrowser x:Name="BrowserForPreviewMarkdown"
                                Grid.Column="2"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Stretch"
                                Navigating="BrowserForPreviewMarkdown_Navigating"
                                Margin="0 10 10 10"/>
    </Grid>
</Window>
